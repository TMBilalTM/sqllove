{
  "name": "SQLLove",
  "short_name": "SQLLove",
  "description": "Sevgilinizle konum paylaşımı ve özel anlar",
  "version": "1.0.0",
  "start_url": "/",
  "id": "/",
  "display": "standalone",
  "orientation": "portrait",
  "background_color": "#ffffff",
  "theme_color": "#ff6b6b",
  "icons": [
    {
      "src": "/icons/android-chrome-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/icons/android-chrome-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/icons/favicon-16x16.png",
      "sizes": "16x16",
      "type": "image/png"
    },
    {
      "src": "/icons/favicon-32x32.png",
      "sizes": "32x32",
      "type": "image/png"
    }
  ],
  "permissions": [
    "geolocation",
    "background"
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "background": {
    "service_worker": "location-worker.js"
  }
}
