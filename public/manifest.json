{
  "name": "SQLLove",
  "short_name": "SQLLove",
  "description": "Sevgililer için konum ve şarj takip uygulaması",
  "version": "1.0.0",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#000000",
  "icons": [
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "permissions": [
    "geolocation",
    "background"
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "background": {
    "service_worker": "location-worker.js"
  }
}
